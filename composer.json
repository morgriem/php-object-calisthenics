{
    "name": "morgriem/php-object-calisthenics",
    "description": "Object calisthenics challenges written in PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gleb Nevzorov",
            "email": "gnevzorov@yandex.ru"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Morgriem\\Calisthenics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morgriem\\Calisthenics\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    }
}
